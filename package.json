{
  "name": "dannyway-bio",
  "version": "1.0.0",
  "description": "Official biography site for dannyway.bio",
  "scripts": {
    "dev": "wrangler pages dev . --compatibility-date=2024-01-01",
    "deploy": "wrangler pages deploy .",
    "build": "echo 'No build step required - static site with Functions'",
    "setup-database": "node setup_database.js",
    "analyze": "node -e \"const { main } = require('./setup_database'); main().then(() => console.log('Analysis complete')).catch(console.error)\""
  },
  "keywords": ["skateboarding", "biography", "cloudflare-pages", "neon"],
  "author": "skateboard.bio network",
  "license": "MIT",
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "wrangler": "^3.0.0"
  }
}
